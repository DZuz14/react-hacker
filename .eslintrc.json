{
  "extends": ["plugin:react/recommended", "airbnb", "airbnb/hooks"],
  "rules": {
    "arrow-body-style": 0,
    "linebreak-style": 0,
    "no-use-before-define": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unescaped-entities": 0,
    "react/jsx-one-expression-per-line": 0,
    "semi": 0,
    "comma-dangle": 0
  }
}
